{
    "ToDoConfig":
    {
        "SortType": "alphabetical",
        "GroupByType": "project",
        "AddCreationDate": true,
        "ListOnStart": true,
        "ListAfterCommand": false,
        "DisplayBeforeThresholdDate": false
    },
    "Serilog":
    {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": "Information",
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Enrich": [ "FromLogContext" ]
    }
}